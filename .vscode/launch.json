{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "extensionHost",
			"name": "Launch",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/packages/vscode-pss-support",
				"--extensionDevelopmentPath=${workspaceFolder}/vscode/zuspec-langserver"
			],
            "preLaunchTask": {
                "type": "npm",
                "script": "compile"
            }
		}
	]
}

